/*Jhat's Side box*/
div.jhat-box.jhat-transparent.jhat-radius
    div.jhat-header.jhat-bg
        div.jhat-banner
            span Jhat
            ul.jhat-icon.jhat-tr-radius
                li.jhat-icon-item.jhat-icon-hide

        div.jhat-sum
            img.jhat-avatar-large(src='/img/avatar/#{user._id}#{user.extName}')
            div.jhat-sum-inner
                div.jhat-nickname=user.name 
                span.jhat-status
                input.jhat-signature(value='#{user.signature || ''}', title='#{user.signature || ''}')

    div.jhat-list-content.jhat-scroll
        ul.jhat-contact-list.jhat-scroll-inner
            -friends.forEach(function(friend) {
                li.jhat-contact-item.jhat-radius(_id='#{friend._id}')
                    img.jhat-contact-avatar(src='/img/avatar/#{friend._id}#{friend.extName}')
                    span.jhat-contact-name: a #{friend.name}
            -});

/* A button for showing the box*/
div.jhat-show.jhat-transparent.jhat-tl-radius.jhat-bl-radius.jhat-transtion
